model maledict_death_cinematic {

	mesh									models/md5/cinematics/maledict_death/maledict.md5mesh

	anim dragon_heart					models/md5/cinematics/maledict_death/dragon_heart.md5anim {
		frame 29								sound_voice		snd_returnwhatisours

		frame 114								trigger func_emitter_cinematic_bloodspew	//shwood
		frame 114								call map_hell::hideFuncEmitterHeartcram		//shwood

		frame	93								sound_global	hd_thrust_stinger
		frame	108								sound_global	hd_hand_thrust
		frame	121								sound_global	hd_dragondeath



		frame 126								trigger func_emitter_cinematic_heartspew	//shwood

		//frame 146								call map_erebus1::bloom_on			//shwood

		frame 146								sound_body		e1_headlift
	}

	anim dragon_heart_flyin				models/md5/cinematics/maledict_death/dragon_heart_flyin.md5anim {
		frame	15						sound_global	hd_dragon_grunt
	}

	anim dragon_heart_closeup				models/md5/cinematics/maledict_death/dragon_heart_closeup.md5anim {
	
	
	}

	anim dragon_death					models/md5/cinematics/maledict_death/dragon_death.md5anim {

		frame	10								sound_global	hd_dscream_01
		frame	22								sound_global	hd_dscream_02

		frame 30								sound_body		snd_wingflap_03
		frame 52								sound_body		snd_wingflap_01
		
		frame	53								sound_global	hd_violin
		
		frame	59								sound_global	hd_death_whoosh
		
		frame 76								sound_body		snd_wingflap_02
		frame 97								sound_body		snd_wingflap_03

		frame 1									call map_hell::hideFuncEmitterHeartSpew		//shwood
		frame 1									trigger func_emitter_deathjuice				//shwood

		frame 45								trigger func_emitter_deathblossom 			//shwood
		frame 45								trigger func_emitter_deathflames 			//shwood

		frame 60								skin	skins/die_mal						//shwood
		frame 60								call	setShaderTime						//shwood

		frame 85								call map_hell::showskullsmoke				//shwood
		frame	85								sound_global	hd_deathscream

		frame	128								sound_global	hd_death_preslam

		//frame 200								call map_erebus1::bloom_off					//shwood

		frame	212							sound_global	hd_deathbounce_01
		frame	236							sound_global	hd_deathbounce_02
		frame	252							sound_global	hd_deathbounce_02
		frame	271							sound_global	hd_deathbounce_03



	}

}

