//		frame 1										trigger func_emitter_player_heart_flyin //shwood
//		frame 1										trigger func_emitter_player_heart_closeup //shwood
//		frame 1										trigger func_emitter_player_heartcram	//shwood

		frame 1										call map_hell::showFuncEmitterHeartFlyin	//shwood
		frame 1										call map_hell::hideFuncEmitterHeartCloseup	//shwood
		frame 1										call map_hell::hideFuncEmitterHeartcram		//shwood

	}
	anim end_player_closeup						models/md5/cinematics/maledict_death/end_player_closeup.md5anim {
		frame	1									sound_global	hd_heartbeat
		frame 1										call map_hell::hideFuncEmitterHeartFlyin	//shwood
		frame 1										call map_hell::showFuncEmitterHeartCloseup	//shwood
	}
	
	anim end_player_heartcram					models/md5/cinematics/maledict_death/end_player_heartcram.md5anim {
		frame 1										call map_hell::hideFuncEmitterHeartCloseup	//shwood
		frame 1										call map_hell::showFuncEmitterHeartcram		//shwood
	}
}

