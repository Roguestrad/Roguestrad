//Mcneil
//////////////////////////////////////////////


model phobos2_cinematic_mcneil {

	mesh								models/md5/cinematics/intro/mcneil.md5mesh

	channel torso						()
	channel legs						(*origin)

	//anim test							models/md5/cinematics/mcneil_cine/test.md5anim
	//anim lipsync						models/md5/cinematics/mcneil_cine/lipsync.md5anim {
	//	frame 12								sound_voice p2_mcneil_meeting1
	//}

	anim mcn_a							models/md5/cinematics/phobos2/mcneil_a.md5anim {
		frame 1								call map_phobos2::mcneil_typing_hide
	}
	anim mcn_b							models/md5/cinematics/phobos2/mcneil_b.md5anim {
		frame	6							sound_body	p2_mcn_swivel 
		frame 39							sound_voice snd_speech_a
	}
	anim mcn_c							models/md5/cinematics/phobos2/mcneil_c.md5anim {
		frame 18							sound_voice snd_speech_b
	}
	anim mcn_d							models/md5/cinematics/phobos2/mcneil_d.md5anim
	anim mcn_e							models/md5/cinematics/phobos2/mcneil_e.md5anim {
		frame	29							sound_body	p2_mnc_chair
	}
	anim mcn_f							models/md5/cinematics/phobos2/mcneil_f.md5anim {
		frame	4							sound_body	player_sounds_footstep
		frame	17							sound_body2	player_sounds_footstep
		frame	27							sound_body	player_sounds_footstep
		frame	35							sound_voice	snd_speech_d
		frame	35							trigger		func_static_54066
		frame	37							sound_body2	player_sounds_footstep
		frame	56							sound_item	p2_mcn_screenbeep
	}
	anim mcn_g							models/md5/cinematics/phobos2/mcneil_g.md5anim {
		
	}
	anim mcn_h							models/md5/cinematics/phobos2/mcneil_h.md5anim {
		frame 14							sound_voice snd_speech_e
		frame	17							sound_global	p2_mcn_loadingdata
		frame 20							call map_phobos2::mcneil_viewer_start
		frame 67							sound_voice snd_speech_f
		frame	75							sound_global	p2_mcn_screenup
		frame	92							sound_voice2	p2_mcn_dataup
		frame	124							sound_voice2	p2_mcn_dataup
		frame	156							sound_voice2	p2_mcn_dataup
		frame	188							sound_voice2	p2_mcn_dataup
	}
	anim mcn_i							models/md5/cinematics/phobos2/mcneil_i.md5anim {
		frame 10							sound_voice snd_speech_g

	}
	anim mcn_j							models/md5/cinematics/phobos2/mcneil_j.md5anim {

		frame 18								sound_voice p2_mcneil_meeting2
		frame 60							call map_phobos2::mcneil_viewer_maps
		frame	60							sound_voice2	p2_mcn_screenup1
		frame	91							sound_voice2	p2_mcn_screenup1
		frame	113							sound_voice2	p2_mcn_screenup1
		frame	138							sound_voice2	p2_mcn_screenup1
		
	}
	anim mcn_k							models/md5/cinematics/phobos2/mcneil_k.md5anim {
		frame 10							sound_voice p2_mcneil_meeting3
		frame 40							call map_phobos2::open_mcneil_door
		frame 120							call map_phobos2::mcneil_typing_show
	}

}

