model erebus5_cloud_cinematic {
	mesh								models/md5/cinematics/erebus5/e5_cloud.md5mesh
	channel torso							()
	channel legs							(*origin)
	anim e5_cloud_cinematic_a			models/md5/cinematics/erebus5/e5_cloud_cinematic_a.md5anim {
		frame 1										call map_erebus5::cloud_hide_primitive
		frame 1										call map_erebus5::cloud_start_mcneil
		frame 24									sound_voice2 marscity_reception_type
	//	frame 63									sound_voice2	snd_mcneil_triggered_full
	}
	anim e5_cloud_cinematic_b			models/md5/cinematics/erebus5/e5_cloud_cinematic_b.md5anim {
		frame 1										call map_erebus5::cloud_start_lockdoor
		frame 2										sound_voice		snd_cloud_triggered1
	}
	anim e5_cloud_cinematic_c			models/md5/cinematics/erebus5/e5_cloud_cinematic_c.md5anim {
		frame 9										sound_body		player_sounds_footstep
		frame 25									sound_body		player_sounds_footstep
	}
	anim e5_cloud_cinematic_d			models/md5/cinematics/erebus5/e5_cloud_cinematic_d.md5anim {
		frame 15									sound_body		snd_mcneil_triggered1
	}
	anim e5_cloud_cinematic_e			models/md5/cinematics/erebus5/e5_cloud_cinematic_e.md5anim {
		frame 14									sound_voice		snd_cloud_triggered2
	}
	anim e5_cloud_cinematic_f			models/md5/cinematics/erebus5/e5_cloud_cinematic_f.md5anim {
		frame 15									sound_body		snd_mcneil_triggered2
	}
	anim e5_cloud_cinematic_g			models/md5/cinematics/erebus5/e5_cloud_cinematic_g.md5anim {
		frame 1										sound_voice2 marscity_reception_type
		frame 18									call map_erebus5::CloudInfluence
		frame 18									sound_body power_surge
		frame 31									sound_body2		player_sounds_footstep
		frame 50									sound_body2		player_sounds_footstep
		frame 67									sound_body2		player_sounds_footstep

	}
	anim e5_cloud_cinematic_h			models/md5/cinematics/erebus5/e5_cloud_cinematic_h.md5anim {
		frame 8										sound_voice		snd_cloud_triggered3
		frame 19									sound_body		player_sounds_footstep
		frame 40									sound_body		player_sounds_footstep
		frame 77									sound_body		snd_mcneil_triggered3
	}
	anim e5_cloud_cinematic_i			models/md5/cinematics/erebus5/e5_cloud_cinematic_i.md5anim {
		frame 1										call map_erebus5::cloud_show_primitive
		frame 1										call map_erebus5_cloud::artifact_scanner_start
		frame 1										call map_erebus5::EmittersOff
		frame 68							sound_voice2 typing
	}
	anim e5_cloud_cinematic_j			models/md5/cinematics/erebus5/e5_cloud_cinematic_j.md5anim {
		frame 1				 						call map_erebus5::cloud_viewer_start
		frame 3										sound_voice		snd_cloud_triggered4
		frame 60									call map_erebus5::cloud_viewer_done
	}
	anim e5_cloud_cinematic_k			models/md5/cinematics/erebus5/e5_cloud_cinematic_k.md5anim
	anim e5_cloud_cinematic_l			models/md5/cinematics/erebus5/e5_cloud_cinematic_l.md5anim {
		frame 8										sound_voice		snd_cloud_triggered5
	}
	anim e5_cloud_cinematic_m			models/md5/cinematics/erebus5/e5_cloud_cinematic_m.md5anim
	anim e5_cloud_cinematic_n			models/md5/cinematics/erebus5/e5_cloud_cinematic_n.md5anim {
		frame 23									sound_body		snd_mcneil_triggered4
	}
	anim e5_cloud_cinematic_o			models/md5/cinematics/erebus5/e5_cloud_cinematic_o.md5anim {
		frame 5										sound_voice		snd_cloud_triggered6
	}
	anim e5_cloud_cinematic_p			models/md5/cinematics/erebus5/e5_cloud_cinematic_p.md5anim
	anim e5_cloud_cinematic_q			models/md5/cinematics/erebus5/e5_cloud_cinematic_q.md5anim {
		frame 61								sound_voice2 type
		frame 63								sound_body2 type
		frame 65								sound_body3 type
