	anim e5_cloud_cinematic_r			models/md5/cinematics/erebus5/e5_cloud_cinematic_r.md5anim
	anim e5_cloud_cinematic_s			models/md5/cinematics/erebus5/e5_cloud_cinematic_s.md5anim
