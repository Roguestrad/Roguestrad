//////////////////////////////////////////////
//IMP
//////////////////////////////////////////////

model erebus1_cinematic_imp {
	mesh							models/md5/cinematics/intro/imp.md5mesh
	channel torso					()
	channel legs					(*origin)

	anim idle						models/md5/cinematics/gravgun_intro/imp_a.md5anim

	anim imp_a						models/md5/cinematics/gravgun_intro/imp_a.md5anim{
		frame 1							call map_erebus1::InitImpLight
	}
	anim imp_b						models/md5/cinematics/gravgun_intro/imp_b.md5anim{
		frame 20								sound_body grabber_imp_sight
		//frame 50								sound_voice grabber_imp_sight2
		frame 60								sound_voice2 grabber_imp_thud
	}
	anim imp_c						models/md5/cinematics/gravgun_intro/imp_c.md5anim {
		frame 4								sound_voice2 grabber_imp_yell
	//	frame 60							trigger_fx fireball fx/implight.fx
		frame 60							call map_erebus1::ImpLightOn
		frame 60							sound_voice grabber_imp_attack
		frame 60							sound_body monster_demon_imp_fireball_create
		frame 60							start_emitter impfireball fireball impfireball2.prt
	}
	anim imp_d						models/md5/cinematics/gravgun_intro/imp_d.md5anim {
		frame 28							sound_body monster_demon_imp_fireball_throw
		frame 28							sound_voice monster_demon_imp_fireball_flight
	}
	anim imp_e						models/md5/cinematics/gravgun_intro/imp_e.md5anim {
		frame 10							sound_body 	grabber_imp_idle
		frame 60							sound_voice2  grabber_imp_idle2
	}
	anim imp_f						models/md5/cinematics/gravgun_intro/imp_f.md5anim
	anim imp_g						models/md5/cinematics/gravgun_intro/imp_g.md5anim {
		frame 1							event preBurn
		frame 2							event burn
		frame 2							sound_voice2 monster_demon_imp_fireball_explode
		frame 2							sound_voice monster_demon_imp_die
		frame 2							sound_body monster_demon_archvile_burn
		frame 5							stop_emitter impfireball
		frame 5							call map_erebus1::ImpLightOff
		//frame 5							start_emitter explosion Back_high burn_imp.prt
	}
	anim imp_h						models/md5/cinematics/gravgun_intro/imp_h.md5anim{
		frame 1							stop_emitter explosion
	}

}

