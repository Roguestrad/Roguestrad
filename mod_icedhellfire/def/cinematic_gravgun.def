//////////////////////////////////////////////

model erebus1_cinematic_marine_gravitygun {

	mesh								models/md5/cinematics/gravgun_intro/marine_gravmesh.md5mesh

	channel torso						()
	channel legs						(*origin)

	anim ggun_a							models/md5/cinematics/gravgun_intro/marine_a.md5anim {
		frame 1								sound_voice grabber_cin_idle
		frame 1								sound_voice2	grabber_marine_breathe
		frame 1								sound_body snd_footstep
		frame 14							sound_body snd_footstep
		//frame 28							sound_body snd_footstep
	}
	anim ggun_b							models/md5/cinematics/gravgun_intro/marine_b.md5anim {
		frame 64								sound_body grabber_marine_huh
	}
	anim ggun_c							models/md5/cinematics/gravgun_intro/marine_c.md5anim
	anim ggun_d							models/md5/cinematics/gravgun_intro/marine_d.md5anim {
		frame 28							sound_body	grabber_marine_grunt
		frame 35							start_emitter gravgun_fx grabfx grabber_fx.prt
		frame 35							sound_body	grabber_cin_mainfire
		frame 35							sound_voice grabber_cin_electro
		frame 35							sound_voice2 grabber_cin_fireloop
		frame 35							start_emitter gravgun_fx_flash grabfx grabber_fx_flash.prt
		frame 35							trigger_fx grabfx fx/grabber_fx.fx
		frame 35							start_emitter beamfx1 grabfx grabber_fx_beam.prt
		frame 36							start_emitter beamfx2 grabfx2 grabber_fx_beam.prt
		frame 37							start_emitter beamfx3 grabfx3 grabber_fx_beam.prt
		frame 36							start_emitter beamfx4 grabfx4 grabber_fx_beam.prt
		frame 35							start_emitter beamfx5 grabfx5 grabber_fx_beam.prt
		frame 36							start_emitter beamfx6 grabfx6 grabber_fx_beam.prt
		frame 37							start_emitter beamfx7 grabfx7 grabber_fx_beam.prt
		frame 36							start_emitter beamfx8 grabfx8 grabber_fx_beam.prt
		frame 35							start_emitter beamfx9 grabfx9 grabber_fx_beam.prt
		frame 37							start_emitter beamfx10 grabfx10 grabber_fx_beam.prt
		frame 35							start_emitter beamfx11 grabfx11 grabber_fx_beam.prt
		frame 36							start_emitter beamfx12 grabfx12 grabber_fx_beam.prt

//		frame 35							start_emitter beamfx13 grabfx13 grabber_fx_beam.prt

		frame 50							start_emitter beam_end grabfx13 grabber_fx2.prt

	}
	anim ggun_e							models/md5/cinematics/gravgun_intro/marine_e.md5anim
	anim ggun_f							models/md5/cinematics/gravgun_intro/marine_f.md5anim {
		frame 1								stop_emitter gravgun_fx_flash
		frame 10							start_emitter gravgun_fx_flash2 grabfx grabber_fx_flash.prt
		frame 10							sound_voice grabber_cin_release
		frame 10							sound_voice2 grabber_cin_silent
		frame 10							sound_body grabber_cin_silent
		frame 15							stop_emitter gravgun_fx
		frame 15							stop_emitter beamfx1
		frame 15							stop_emitter beamfx2
		frame 15							stop_emitter beamfx3
		frame 15							stop_emitter beamfx4
		frame 15							stop_emitter beamfx5
		frame 15							stop_emitter beamfx6
		frame 15							stop_emitter beamfx7
		frame 15							stop_emitter beamfx8
		frame 15							stop_emitter beamfx9
		frame 15							stop_emitter beamfx10
		frame 15							stop_emitter beamfx11
		frame 15							stop_emitter beamfx12
//		frame 15							stop_emitter beamfx13
		frame 15							stop_emitter beam_end

	}
	anim ggun_g							models/md5/cinematics/gravgun_intro/marine_g.md5anim {
		frame 1								stop_emitter gravgun_fx_flash2

	}
	anim ggun_h							models/md5/cinematics/gravgun_intro/marine_h.md5anim {
		frame	14							sound_voice		e1_fall_01
		frame	25							sound_voice2	e1_fall_02
	}

}

